#include <iostream>
using namespace std;
int main()
{
    string a;
    int b, c, w=0, tab[10];
    while(cin>>a>>b>>c)
    {
 
 
        if (a=="+")
        {
            w=tab[b]+tab[c];
            cout<<w<<endl;
        }
        else if(a=="-")
        {
            w=tab[b]-tab[c];
            cout<<w<<endl;
        }
        else if(a=="*")
        {
            w=tab[b]*tab[c];            cout<<w<<endl;
        }
        else if(a=="/")
        {
            w=tab[b]/tab[c];
            cout<<w<<endl;
        }
        else if(a=="%")
        {
            w=tab[b]%tab[c];
            cout<<w<<endl;
        }
        else if(a=="z")
        {
            tab[b]=c;
            cout<<""<<endl;
        }
    }
    return 0;
}
 
